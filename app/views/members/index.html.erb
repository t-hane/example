<%= link_to '新規作成', new_member_path %>
<%= link_to 'CSV', members_path(format: 'csv') %>
<h1>メンバー一覧</h1>
<table>
    <thead>
        <tr><th>名前</th><th>メール</th><th>年齢</th><th>詳細</th></tr>
    </thead>
    <tbody>
        <% @members.each do |member| %>
            <tr>
                <td><%= member.name %></td>
                <td><%= member.email %></td>
                <td><%= member.age %></td>
                <td><%= link_to '詳細', member_path(member) %></td>
            </tr>
        <% end %>
    </tbody>
</table>
